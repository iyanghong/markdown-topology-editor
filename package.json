{
  "name": "markdown-topology-editor",
  "version": "0.1.4",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest app ./src/index.js",
    "wc": "vue-cli-service build --mode production --target wc --name markdown-topology-editor --dest wc ./src/index.js",
    "wc-async": "vue-cli-service build --mode production --target wc-async --name markdown-topology-editor --dest wc-async ./src/index.js",
    "lib": "vue-cli-service build --target lib --name markdown-topology-editor --dest lib ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "mavon-editor": "^2.9.1",
    "element-ui": "^2.14.1",
    "echarts": "^4.9.0",
    "eslint": "^6.7.2",
    "@topology/activity-diagram": "^0.3.0",
    "@topology/chart-diagram": "^0.5.1",
    "@topology/class-diagram": "^0.3.0",
    "@topology/core": "^0.5.23",
    "@topology/flow-diagram": "^0.3.0",
    "@topology/layout": "^0.3.0",
    "@topology/sequence-diagram": "^0.3.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "vue-json-viewer": "^2.2.19",
    "file-saver": "^2.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "description": "一边写markdown的时候嵌入在线拓扑绘图",
  "entry" : "src/index.js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyanghong/markdown-topology-editor.git"
  },
  "keywords": [
    "topology",
    "markdown",
    "editor"
  ],
  "author": "iyanghong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iyanghong/markdown-topology-editor/issues"
  },
  "homepage": "https://github.com/iyanghong/markdown-topology-editor#readme"
}
